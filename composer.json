{
    "name": "jwilkinson/collections",
    "description": "A package providing a wide variety of standard interfaces and datastructures",
    "keywords": [
        "collection",
        "datastructure"
    ],
    "homepage": "https://github.com/john-wilkinson/collections",
    "license": "MIT",
    "authors": [
        {
            "name": "John Wilkinson",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Jmw\\Collection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmw\\Collection\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
